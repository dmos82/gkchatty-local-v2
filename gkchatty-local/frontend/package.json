{
  "name": "@gkchatty/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "copy-pdf-worker": "node scripts/prepare-pdf-worker.js",
    "dev": "pnpm copy-pdf-worker && NODE_TLS_REJECT_UNAUTHORIZED=0 next dev -p 4003 -H 0.0.0.0",
    "dev:custom": "NODE_TLS_REJECT_UNAUTHORIZED=0 node server.mjs",
    "build": "next build",
    "build:docker": "cross-env NEXT_PUBLIC_API_BASE_URL=$NEXT_PUBLIC_API_BASE_URL next build",
    "start": "next start -p 4003",
    "lint": "next lint"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "1.1.7",
    "@radix-ui/react-avatar": "1.1.4",
    "@radix-ui/react-dialog": "1.1.7",
    "@radix-ui/react-dropdown-menu": "2.1.7",
    "@radix-ui/react-label": "2.1.3",
    "@radix-ui/react-progress": "1.1.4",
    "@radix-ui/react-scroll-area": "1.2.5",
    "@radix-ui/react-select": "2.2.4",
    "@radix-ui/react-slot": "1.2.0",
    "@radix-ui/react-switch": "1.1.4",
    "@radix-ui/react-tabs": "1.1.4",
    "@radix-ui/react-toast": "1.2.7",
    "@radix-ui/react-tooltip": "1.2.0",
    "@types/lodash.debounce": "4.0.9",
    "autoprefixer": "10.4.21",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "framer-motion": "12.11.0",
    "js-cookie": "3.0.5",
    "lodash.debounce": "4.0.8",
    "lucide-react": "0.487.0",
    "next": "14.2.28",
    "next-auth": "4.24.11",
    "next-themes": "0.4.6",
    "pdfjs-dist": "4.8.69",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-pdf": "9.2.1",
    "react-swipeable": "7.0.2",
    "react-toastify": "9.1.3",
    "sharp": "0.34.2",
    "tailwind-merge": "3.2.0",
    "tailwindcss-animate": "1.0.7",
    "tesseract.js": "6.0.1",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@types/js-cookie": "3.0.6",
    "@types/node": "20.17.30",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "copy-webpack-plugin": "13.0.0",
    "cross-env": "7.0.3",
    "cssnano": "7.0.7",
    "eslint": "8",
    "eslint-config-next": "14.2.28",
    "express": "4.18.2",
    "http-proxy-middleware": "3.0.5",
    "postcss": "8.4.33",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          ".next/**"
        ],
        "inputs": [
          "!lint"
        ],
        "env": [
          "NODE_ENV"
        ],
        "cache": false
      }
    }
  },
  "description": "GKChatty Web Frontend - Next.js interface for RAG platform",
  "packageManager": "pnpm@8.15.0"
}
