  "version": "1.0.0",
  "private": true,
  "description": "GKChatty Local Backend API - RAG platform with SQLite, ChromaDB, Transformers.js",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --notify=false src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "load-kb": "ts-node src/scripts/loadSystemKnowledge.ts",
    "reindex:tenant-kb": "ts-node src/scripts/reindex-tenant-kb.ts",
    "reindex:all-kb": "ts-node src/scripts/reindex-system-tenant-kb.ts",
