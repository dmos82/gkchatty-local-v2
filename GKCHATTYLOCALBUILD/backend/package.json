    "kbs:verify-tenant-docs": "ts-node src/scripts/verify-tenant-kb-documents.ts",
    "rag:verify-search": "ts-node src/scripts/verify-search-modes.ts",
    "user:create-admin": "ts-node src/scripts/create-admin-user.ts",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test:openai": "ts-node src/test-openai.ts",
    "test": "dotenv -e .env.test -- jest --coverage --runInBand",
    "test:watch": "dotenv -e .env.test -- jest --watch",
    "test:video": "dotenv -e .env.test -- jest src/services/__tests__/videoProcessor.test.ts --coverage"
  },
